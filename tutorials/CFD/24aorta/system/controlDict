FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     icoFoam; //icoFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         3;//35;

deltaT          0.00001;

writeControl    adjustableRunTime; //timeStep;

writeInterval  0.01;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo 0.9;


// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

